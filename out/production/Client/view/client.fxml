<BorderPane minHeight="600.0" minWidth="606.0" prefHeight="600.0" prefWidth="606.0" style="-fx-background-color: #a295ff;" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.UserController">
    <top>
