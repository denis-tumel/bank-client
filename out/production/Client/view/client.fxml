                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
            </TableView>
         </children></AnchorPane>
    </center>
    <left>
        <AnchorPane id="leftBorder" prefHeight="342.0" prefWidth="184.0" BorderPane.alignment="CENTER">
         <children>
            <Label fx:id="accountValue" layoutX="40.0" layoutY="3.0" prefHeight="23.0" prefWidth="214.0" text="На вашем счету: ">
