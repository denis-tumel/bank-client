                <TextField fx:id="firstName" layoutX="88.0" layoutY="115.0" prefHeight="40.0" prefWidth="320.0" promptText="Имя" AnchorPane.leftAnchor="88.0" AnchorPane.rightAnchor="88.0" />
                <Button cancelButton="true" layoutX="343.0" layoutY="36.0" mnemonicParsing="false" onAction="#ActionBack" prefHeight="25.0" prefWidth="60.0" text="Назад" textFill="#8d5959">
                    <font>
                        <Font name="Simplex" size="12.0" />
                    </font></Button>
                <Label layoutX="88.0" layoutY="36.0" text="Регистрация">
                    <font>
                        <Font name="Micra" size="24.0" />
                    </font>
                </Label>
                <TextField fx:id="emailField" layoutX="89.0" layoutY="169.0" prefHeight="40.0" prefWidth="320.0" promptText="e-mail" />
                <PasswordField fx:id="passwordField" layoutX="89.0" layoutY="225.0" prefHeight="40.0" prefWidth="320.0" promptText="пароль" />
