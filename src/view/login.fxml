<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox id="VBoxLogin" prefHeight="352.0" prefWidth="380.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.LoginController">
    <children>
        <AnchorPane id="AnchorPane" fx:id="registerLabel" prefHeight="383.0" prefWidth="425.0" style="-fx-background-color: #a295ff;">
            <children>
                <Button defaultButton="true" layoutX="109.0" layoutY="282.0" mnemonicParsing="false" onAction="#ActionLogin" prefHeight="35.0" prefWidth="85.0" text="Войти" textFill="#b95959">
                    <font>
                        <Font name="Sitka Small" size="12.0" />
                    </font></Button>
                <TextField id="text-field" fx:id="emailField" layoutX="40.0" layoutY="125.0" prefHeight="35.0" prefWidth="338.0" promptText="E-mail" />
                <Label layoutX="115.0" layoutY="30.0" prefHeight="35.0" prefWidth="255.0" text="Авторизация" textAlignment="CENTER">
                    <font>
                        <Font name="Micra" size="24.0" />
                    </font>
                </Label>
                <PasswordField fx:id="passwordField" layoutX="43.0" layoutY="195.0" prefHeight="35.0" prefWidth="338.0" promptText="Пароль" />
                <Button cancelButton="true" layoutX="316.0" layoutY="35.0" mnemonicParsing="false" onAction="#ActionBack" prefHeight="25.0" prefWidth="60.0" text="Назад" textFill="#8d5959">
                    <font>
                        <Font name="Simplex" size="12.0" />
                    </font>
                </Button>
                <Button layoutX="242.0" layoutY="282.0" mnemonicParsing="false" onAction="#ActionRegister" prefHeight="35.0" prefWidth="85.0" text="Регистрация" textFill="#8d5959" />
            <Separator layoutX="-1.0" layoutY="84.0" prefWidth="426.0" />
            </children>
        </AnchorPane>
    </children>
</VBox>
